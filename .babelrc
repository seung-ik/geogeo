{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true
      }
    ],
    [
      "module-resolver",
      {
        "root": [
          "."
        ],
        "extensions": [
          ".ts",
          ".tsx"
        ],
        "alias": {
          "@home": "./src/components/home",
          "@virtual": "./src/components/virtual",
          "@components": "./src/components",
          "@styles": "./src/styles",
          "@service": "./src/service",
          "@store": "./src/store",
          "@actions": "./src/store/actions",
          "@mock": "./__mock__"
        }
      }
    ]
  ]
}
